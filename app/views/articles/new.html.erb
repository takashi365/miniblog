
      <div class="row">
          <%= form_with model: @article, local: true, method: :post, class: "col s6 offset-m3" do |f|%>
            <div class="row">
              <div class="input-field col s6">
                <%= f.text_field :title %>
                <label for="input_text">タイトル</label>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <%= f.text_field :article %>
                <label for="textarea1">本文</label>
              </div>
            </div>
            <button class="btn waves-effect waves-light" type="submit" name="action">送信
                <i class="material-icons right"></i>
              </button>
          <% end %>
        </div>

    <script type="text/javascript" src="./app.js"></script>